{
  "name": "apollo-cli",
  "description": "CLI for the Apollo platform of tooling",
  "version": "0.0.0",
  "author": "opensource@apollographql.com",
  "bin": {
    "apollo": "./bin/run"
  },
  "bugs": "https://github.com/apollographql/apollo-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.4.21",
    "@oclif/config": "^1.6.17",
    "@oclif/plugin-help": "^1.2.10"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.4.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.11",
    "globby": "^8.0.1",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/apollographql/apollo-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "apollo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "apollographql/apollo-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p . --noEmit",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
