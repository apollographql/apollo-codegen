{
  "name": "apollo-codegen",
  "version": "0.9.6",
  "description": "Generate client code based on a GraphQL schema and query documents",
  "main": "./lib/index.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "compile": "tsc && babel temp --out-dir lib --source-maps && rm -rf temp",
    "watch": "concurrently \"tsc --watch\" \"babel temp --out-dir lib --watch --source-maps\"",
    "prepublish": "rm -rf lib temp && npm run compile",
    "test": "mocha --recursive --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "apollostack/apollo-codegen"
  },
  "author": "Martijn Walraven <martijn@martijnwalraven.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/graphql": "^0.8.2",
    "@types/node": "^6.0.52",
    "@types/node-fetch": "^1.6.6",
    "@types/yargs": "^6.3.3",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "common-tags": "^1.3.0",
    "concurrently": "^3.1.0",
    "glob": "^7.0.5",
    "mocha": "^3.1.2",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "change-case": "^3.0.0",
    "graphql": "^0.8.1",
    "inflected": "^1.1.7",
    "node-fetch": "^1.5.3",
    "source-map-support": "^0.4.2",
    "yargs": "^6.4.0"
  }
}
